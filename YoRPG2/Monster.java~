public class Monster extends Character{

    //--------Instance Vairables--------------

    //---------Constructors-------------------
    public Monster() {
	health = 150;
	strength = (int) ( Math.random() * 45 + 20 );
	defense = 20;
	atkRating = 1;
    }

    //---------Methods------------------------
    //checks if monster has been slayed yet or not

}
	
